// Prisvaivanie.cpp: Присвание значения вектору строк 

#include "stdafx.h"
#include <iostream>
#include <vector>
#include <string>

using namespace std;

int main() {
	vector<string> w = { "a", "b", "c" };
	vector<string> v;

	v = w;
	v[0] = "d";
	cout << w[0] << " " << w[1] << endl;
	cout << v[0] << " " << v[1] << endl;

	getchar();
	return 0;
}
